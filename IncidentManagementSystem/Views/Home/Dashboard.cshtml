@model IncidentManagementSystem.Model.SearchByDateDto
@*@{
        ViewBag.Title = "Dashboard";
    }

    <h2>Dashboard</h2>*@
<div class="container-fluid dashboard"> 
    <div class="col-lg-12 header ">
        <div class="header-text">
            Text
        </div>
        <div class="col text">
            <span class="page input-icon">Text</span>

            <span class="detail">Text </span>
        </div>
    </div>

    @using (Html.BeginForm("Dashboard", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <div class="container-fluid search-box">
            <div class=" row s-box">


                <div class="col-lg-4 form-group s-dash">
                    @Html.TextBoxFor(model => model.SearchFor, new { @class = "form-control", placeholder = "Search" })
                </div>

                <div class="col-lg-4 s-dash">
                    <input type="submit" value="Search" class="btn btn-primary btn-width" />
                </div>

                <div class="col-lg-4 s-dash">
                    <input type="submit" value="Create" class="btn btn-success btn-width" />
                </div>
            </div>

        </div>
    }
</div>